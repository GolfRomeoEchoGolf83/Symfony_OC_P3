{%  extends "BookingBundle::layout.html.twig" %}

{%  block title %}
{%  endblock %}

{%  block body %}
    <h3>Commande de billets</h3>
    <div class="ticketing">
        {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
        {{ form_errors(form) }}
        {{ form_row(form.visitingDay) }}
        {{ form_row(form.kindOfTicket) }}
        {{ form_row(form.emailVisitor) }}
        {{ form_row(form.nbTicket) }}
        {{ form_row(form.ordering) }}

    </div>

    {# Script jQuery #}
{%  endblock %}

{%  block javascripts  %}
    <script>
        $( function() {
                $( "#bookingbundle_booking_visitingDay").datepicker();
            }
        );
    </script>

{% endblock %}


